using System;
using System.Collections.Generic;


namespace Listas
{
    class Program
    {
        static void Main(string[] args)
        {
            var lista = new List<int>();
            lista.Add(10);
            lista.Add(20);
            lista.Add(30);
            lista.ForEach(i => Console.WriteLine(i));

            Console.ReadLine();
        }
    }
}
