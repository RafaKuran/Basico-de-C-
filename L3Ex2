using System;

namespace L3Ex2
{
    class Program
    {
        static void Main(string[] args)
        {
            int soma = 0;

            for (int i = 1; i <= 100; i++)
            {
                soma = soma + i;
            }

            Console.Write($"Soma = {soma}");
            Console.ReadLine();
        }
    }
}
