using System;
					
public class Program
{
	public static void Main()
	{
		int raio, altura;
		double area;
		
		Console.Write("Qual o raio da caixa: ");
		raio = Convert.ToInt32(Console.ReadLine());
		Console.Write("Qual a altura da caixa: ");
		altura = Convert.ToInt32(Console.ReadLine());
		
		area =  altura * Math.PI  * Math.Pow(raio, 2);		
		
		Console.Write("A área da caixa cilíndrica será {0} cm.", area.ToString("N2"));
	}
}
